# Copy to .env and fill in your values

# Environment Configuration
ENVIRONMENT=dev  # Options: dev, prod (dev uses omi-dev bucket, prod uses omi bucket)

# Groq API Configuration
GROQ_API_KEY=your_groq_api_key_here

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your_account_id_here
R2_ACCESS_KEY_ID=your_access_key_id_here
R2_SECRET_ACCESS_KEY=your_secret_access_key_here
# Buckets are auto-selected based on ENVIRONMENT:
# dev -> omi-dev
# prod -> omi

# Processing Configuration
BATCH_DURATION_SECONDS=120  # 2 minutes
MAX_BATCH_SIZE_MB=20        # Stay under Groq's 25MB limit

# Server Configuration
PORT=8000
HOST=0.0.0.0